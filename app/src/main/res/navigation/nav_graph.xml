<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/categoryListFragment">

    <fragment
        android:id="@+id/categoryListFragment"
        android:name="com.example.personalitytypes.ui.CategoryListFragment"
        android:label="fragment_category_list"
        tools:layout="@layout/fragment_category_list" >
        <action
            android:id="@+id/action_categoryListFragment_to_personalityListFragment"
            app:destination="@id/personalityListFragment" />
    </fragment>
    <fragment
        android:id="@+id/personalityListFragment"
        android:name="com.example.personalitytypes.ui.PersonalityListFragment"
        android:label="fragment_personality_list"
        tools:layout="@layout/fragment_personality_list" />
</navigation>